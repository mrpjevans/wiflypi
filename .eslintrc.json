{
	"env": {
		"browser": true
	},
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": "warn",
		"require-await": 0,
		"@typescript-eslint/no-unused-vars": [
			"error",
			{
				"args": "after-used",
				"ignoreRestSiblings": false,
				"argsIgnorePattern": "^_"
			}
		]
	}
}